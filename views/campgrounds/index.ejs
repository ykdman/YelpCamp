<% layout('layouts/boilerplate') %>

<h1>All CampGrounds</h1>
<div>
    <a href="/campgrounds/new">Create Camp!</a>
</div>

<ul>
    <% for(let campground of campgrounds){ %>
        <li>
            <a href="/campgrounds/<%= campground.id %>" ><%= campground.title %></a>
        </li>
    <% } %>
</ul>
